<%- include("templates/cabecera", {tituloWeb: "Nuevo Futbolista"}) %>

<div class="container">
    <h2>Agregar nuevo futbolista</h2>
    <a href="/" class="btn btn-dark btn-block my-2"> Volver</a>
    <form action="/" class="my-2" method="POST">
    <input type="number" placeholder="Id" class="form-control my-2" name="id">
    <input type="text" placeholder="Nombre" class="form-control my-2" name="nombre">
    <input type="text" placeholder="Equipo" class="form-control my-2" name="equipo">
    <input type="text" placeholder="Liga" class="form-control my-2" name="liga">
    <input type="text" placeholder="Nacionalidad" class="form-control my-2" name="nacionalidad">
    <input type="number" placeholder="Goles" class="form-control my-2" name="goles">
    <button type="submit" class="btn btn-dark btn-block">Agregar</button>
    </form>
</div>
